{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Elasticsearch": {
      "Enabled": false,
      "Url": "http://elasticsearch:9200"
    },
    "File": {
      "Enabled": true,
      "Path": "logs/log.txt"
    }
  },
  "MinServiceDelay": 1000,
  "MaxServiceDelay": 1000
}
