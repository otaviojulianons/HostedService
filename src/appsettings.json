{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Elasticsearch": {
      "Enabled": false,
      "Url": "http://elasticsearch:9200"
    },
    "File": {
      "Enabled": false,
      "Path": "logs/log.txt"
    }
  },
  "WorkerConfig": {
    "MinServiceDelay": 1000,
    "MaxServiceDelay": 1100
  }
}
