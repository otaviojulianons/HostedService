{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Elasticsearch": {
      "Enabled": false,
      "Url": "http://elasticsearch:9200",
      "Path": "logs/serilog.txt"
    },
    "File": {
      "Enabled": false,
      "Path": "logs/log.txt"
    }
  },
  "WorkerConfig": {
    "MinServiceDelay": 1000,
    "MaxServiceDelay": 1100,
    "StressTest": false,
    "StressTestDelay": 1,
    "StressTestCores": 1
  }
}
